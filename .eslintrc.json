{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": [
    "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "max-len": ["error", { "code": 90 }],
    "no-unused-vars": ["off"],
    "@typescript-eslint/no-unused-vars": "error"
  }
}
